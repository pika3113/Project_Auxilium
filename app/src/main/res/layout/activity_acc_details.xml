<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Loading_into_app"
    android:background="#ebebeb">



    <ImageView
        android:layout_width="345dp"
        android:layout_height="655dp"
        android:src="@drawable/rectangle_reg"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"/>

    <Button
        android:id="@+id/logout"
        android:layout_width="220dp"
        android:layout_height="80dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="585dp"
        android:textColor="#707070"
        android:backgroundTint="#f5f5f5"
        android:textSize="34dp"
        android:fontFamily="@font/montserrat_medium"
        android:textStyle="bold"
        android:text="Logout"
        android:textAllCaps="false"
        app:cornerRadius="18dp"
        android:letterSpacing="0.03"
        android:stateListAnimator="@null"/>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/homepgpfp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="60dp"/>
    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@drawable/ic_baseline_person_24"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="65dp"/>
    <TextView
        android:id="@+id/userh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="user123"
        android:textColor="#a3a3a3"
        android:textSize="36dp"
        android:layout_marginTop="185dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="55dp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Username: "
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="260dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="55dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="310dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="55dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone no.:"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="360dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="55dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Post Count:"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="410dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="55dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Help Count:"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="460dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="55dp"/>
    <TextView
        android:id="@+id/changepasswordpop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Change Password"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="518dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_centerHorizontal="true"/>


    <TextView
        android:id="@+id/users"
        android:layout_width="266dp"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:text="User123"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="260dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="70dp"/>
    <TextView
        android:id="@+id/email"
        android:layout_width="266dp"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:text="user@gmail.com"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="310dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="70dp"/>
    <TextView
        android:id="@+id/phoneno"
        android:layout_width="266dp"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:text="12345678"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="360dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="70dp"/>


    <TextView
        android:id="@+id/post"
        android:layout_width="266dp"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:text="10"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="410dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="70dp"/>
    <TextView
        android:id="@+id/help"
        android:layout_width="266dp"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:text="5"
        android:textColor="#a3a3a3"
        android:textSize="20dp"
        android:layout_marginTop="460dp"
        android:fontFamily="@font/montserrat_medium"
        android:layout_marginLeft="70dp"/>




    <ImageView
        android:id="@+id/userpop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_baseline_arrow_forward_ios_24"
        android:layout_marginLeft="342dp"
        android:layout_marginTop="267dp"/>
    <ImageView
        android:id="@+id/emailpop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_baseline_arrow_forward_ios_24"
        android:layout_marginLeft="342dp"
        android:layout_marginTop="317dp"/>
    <ImageView
        android:id="@+id/nopop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_baseline_arrow_forward_ios_24"
        android:layout_marginLeft="342dp"
        android:layout_marginTop="367dp"/>

    <ImageView
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_baseline_arrow_back_24"
        android:layout_margin="6dp"/>








</RelativeLayout>